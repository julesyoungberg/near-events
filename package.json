{
    "name": "starter--near-sdk-as",
    "version": "0.0.1",
    "description": "Start with a basic project",
    "scripts": {
        "dev": "watch -d -n 1 'clear && npm run test'",
        "test:unit": "npx asp -f .spec",
        "test:simulate": "cargo test -- --nocapture",
        "test": "npm run test:unit && npm run test:simulate",
        "clean": "rm -rf ./build && rm -rf ./neardev",
        "build": "asb --target debug",
        "build:release": "asb",
        "asp": "asp --verbose --nologo",
        "prettier": "npx prettier --write ./src"
    },
    "keywords": [],
    "author": "hello@near.org",
    "license": "ISC",
    "devDependencies": {
        "near-sdk-as": "^3.1.0",
        "prettier": "^2.7.1"
    },
    "dependencies": {
        "@assemblyscript/loader": "^0.20.11"
    }
}
